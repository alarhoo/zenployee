<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.f"
	controllerName="zenployee.controller.FlexibleColumnLayout"
	displayBlock="true"
	height="100%">
	<FlexibleColumnLayout
		id="flxColumnLayout"
		autoFocus="false"
		stateChange="onStateChanged"
		restoreFocusOnBackNavigation="true"
		layout="{/layout}"
		backgroundDesign="Translucent" />
</mvc:View>
