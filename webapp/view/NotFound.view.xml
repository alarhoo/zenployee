<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="zenployee.controller.NotFound">
	<IllustratedMessage
		illustrationType="sapIllus-PageNotFound"
		title="{i18n>NotFound}"
		description="{i18n>NotFound.description}">
		<additionalContent>
			<Button
				text="{i18n>NotFound.backToHome}"
				press=".onNavBack" />
		</additionalContent>
	</IllustratedMessage>
</mvc:View>
